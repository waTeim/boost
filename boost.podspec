  s.screenshots  = 
  s.license      = 'BOOST'
  s.author       = { "waTeim" => "truthset@gmail.com" }
  s.source       = { :git => "https://github.com/waTeim/boost.git", :tag => s.version.to_s }

  s.platform     = :osx, '10.6'
